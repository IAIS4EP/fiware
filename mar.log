commit 7ff75b6507efd7562d37d22aa230abd880f32280
Author: marcoantonelli <info@marcoantonelli.com>
Date:   Wed Nov 18 22:44:14 2015 +0100

    Update smoketest.sh
    
    added some POST and GET tests

diff --git a/ge_orion/ge_spagobi/smoketest.sh b/ge_orion/ge_spagobi/smoketest.sh
index 3bf7ade..b9044fa 100644
--- a/ge_orion/ge_spagobi/smoketest.sh
+++ b/ge_orion/ge_spagobi/smoketest.sh
@@ -1,4 +1,31 @@
-# add your smoketest here - e.g. curl based API test of a GE
+# remember to change $HOST variable 
+# (if you have use docker you can set localhost:1026)
 
-HOST=$1
-PORT=$2
+# check if daemon is running
+curl $HOST/version
+
+#create a first "Machine1" Entity with two attributes: temperature and pressure
+curl -v -H "Accept: application/json" -H "Content-type: application/json" -X POST -d '
+  { "attributes": 
+    [ 
+      { "name": "temperature", 
+      "type": "float", 
+      "value": "23" 
+      },
+      { "name": "pressure", 
+      "type": "integer", 
+      "value": "720" 
+      }
+    ]
+  }' $HOST/v1/contextEntities/Machine1
+
+#retrieve "Machine1" attributes
+curl $HOST/v1/contextEntities
+
+#update the value of the temperature attribute in "Machine1" 
+curl -v -H "Accept: application/json" -H "Content-type: application/json" -X PUT -d '{
+    "value": "18.4"
+}' $HOST/v1/contextEntities/Machine1/attributes/temperature
+
+#then check that the new value is correctly updated
+curl $HOST/v1/contextEntities

